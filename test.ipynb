   "source": [
    "import torch\n",
    "# 创建一个张量并设置requires_grad为True\n",
    "x = torch.tensor([0., 1., 2., 3.], requires_grad=True)\n",
    "\n",
    "# 进行一些操作\n",
    "y = x.pow(2).sum()\n",
    "\n",
    "# 计算梯度\n",
    "y.backward()\n",
    "\n",
    "# 打印梯度\n",
    "print(x.grad)"
   ]
